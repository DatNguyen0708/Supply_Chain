
 <% include header%>
    <script src="javascripts/addAction.js"></script>




<!-- Page Content -->
    <input type="hidden" value="<%= add %>" id="add">
    <input type="hidden" value="<%= account %>" id="account">
    <div class="container" style="margin-top: 100px;">
	    <div class="row">
	      <div class="col-md-2"></div>
	      <div class="col-md-8"> 
	        <h1  style="color: red;" align="center">Tranfer Ownership For This Product</h1>
	        <br></br>
	        <div class="box-body padding-large">
	          <div class="form-group row">
	            <label for="executefrom " class="col-md-3 col-xs-12 text-left">Execute From</label>
                <input type="text" name="executefrom" class="col-md-9 col-xs-12 form-control" id="x" value="<%= account %>" disabled>        
	          </div>
	        </div>
	        <div class="box-body padding-large">
	          <div class="form-group row">
	            <label for="password" class="col-md-3 col-xs-12 text-left">Password</label>
                <input type="password" name="password" class="col-md-9 col-xs-12 form-control" id="password" placeholder="Password">
	          </div>
	        </div>
	        <div class="box-body padding-large">
	          <div class="form-group row">
	            <label for="amount" class="col-md-3 col-xs-12 text-left">Amount</label>
                <input type="number" name="amount" class="col-md-9 col-xs-12 form-control" id="amount" placeholder="Amount...">
	          </div>
	        </div>
	        <div class="box-body padding-large">
	          <div class="form-group row">
	            <label for="to" class="col-md-3 col-xs-12 text-left">To</label>
                <select class="col-md-9 col-xs-12 form-control" id="comboboxAccount"></select>
	          </div>
	        </div>	        
	        <div class="box-body padding-large">
	          <div class="form-group row">
	            <label class="col-md-5 col-xs-12 text-left"></label>
	            <button type="submit " class="col-md-2 col-xs-12 btn btn-primary " onclick="submit();">Submit</button>
	            <label class="col-md-5 col-xs-12 text-left"></label>
	          </div>
	        </div>
	      <div class="col-md-2"></div>
	    </div>
  	</div>
  </div>



 <% include footer%>